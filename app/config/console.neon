services:
	#commands
	deleteCache:
		class: Console\Command\DeleteCache
		tags: [consoleCommand]

	install:
		class: Console\Command\Install
		tags: [consoleCommand]

	#helpers
	dialog:
		class: \Symfony\Component\Console\Helper\DialogHelper
		tags: [consoleHelper]

	progress:
		class: \Symfony\Component\Console\Helper\ProgressHelper
		tags: [consoleHelper]

	containerHelper:
		class: \ContainerHelper(@container)
		tags: [consoleHelper]